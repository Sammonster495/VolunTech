{
  "expo": {
    "name": "voluntech",
    "slug": "voluntech",
    "version": "1.0.0",
    "orientation": "portrait",
    "assets": [
      "./assets/fonts/*"
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "icon": "./assets/images/mainlogo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mainlogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sammonster495.voluntech",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBqeTB4Vbev342dA6b4PWZf-H3S1QTZyrM"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "82b74bab-bfb5-4318-aecd-a8931c3bc373"
      }
    },
    "scheme": "voluntech"
  }
}
